sysName <%= @data['sysname'] %> 
sysLocation <%= @data['syslocation'] %> 
sysContact <%= @data['syscontact'] %>
sysServices 76

<% @data['usmuser'].each do |user| %>
<%= @data[user]['acl'] %> <%= @data[user]['usmuser'] %> <%= @data[user]['secmode'] %>
<% end %>
skipNFSInHostResources true
storageUseNFS 1
dontLogTCPWrappersConnects true
